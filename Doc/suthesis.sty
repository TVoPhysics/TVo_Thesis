	\def\beforepreface{
		\ifdblabstract   % the top page is a good place for this
		\pagestyle{empty}   
		\abstract \endabstract \newpage
		\fi
		\pagenumbering{roman}
		\pagestyle{plain}
		% A & B the title page and the abstract
		\ifelectronicsubmit
		\abstract \endabstract \newpage
		\manuscriptp	
		\else
		\officialaprovp
		\abstract \endabstract \newpage
		\fi
		% C the OPTIONAL copy right page
		\ifcopyright\copyrightpage\fi
		% D the Table of Contents
		\newpage
		\tableofcontents
		\newpage
		\iftablespage
		{\addvspace{10pt}
			\let\saveaddvspace=\addvspace
			\def\addvspace##1{}
			\listoftables
			\addcontentsline{toc}{chapter}{List of Tables}
			\let\addvspace=\saveaddvspace}
		\newpage
		\fi
		\iffigurespage
		{\addvspace{10pt}
			\let\saveaddvspace=\addvspace
			\def\addvspace##1{}
			\listoffigures
			\addcontentsline{toc}{chapter}{List of Figures}
			\let\addvspace=\saveaddvspace}
		\newpage
		\fi
	}
	
	\def\prefacesection#1{%
		\chapter*{#1}%
